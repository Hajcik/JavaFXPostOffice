<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" prefHeight="265.0" prefWidth="445.0" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="265.0" prefWidth="600.0">
         <children>
            <ListView fx:id="listViewPeople" layoutX="14.0" layoutY="20.0" onMouseClicked="#onClickListViewCell" prefHeight="204.0" prefWidth="232.0" />
            <Button fx:id="aboutBtn" layoutX="279.0" layoutY="127.0" mnemonicParsing="false" onAction="#setAboutBtn" prefHeight="41.0" prefWidth="147.0" text="About">
               <font>
                  <Font name="Arial" size="18.0" />
               </font></Button>
            <Button fx:id="addAddressBtn" layoutX="279.0" layoutY="21.0" mnemonicParsing="false" onAction="#onOpenDialog" prefHeight="60.0" prefWidth="147.0" text="Add Address">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font></Button>
            <Button fx:id="exitBtn" layoutX="279.0" layoutY="180.0" mnemonicParsing="false" onAction="#exitBtn" prefHeight="41.0" prefWidth="147.0" text="Exit">
               <font>
                  <Font name="Arial Bold" size="17.0" />
               </font></Button>
         </children></Pane>
   </children>
</GridPane>
